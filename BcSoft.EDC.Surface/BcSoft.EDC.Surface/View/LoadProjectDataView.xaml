<dx:DXWindow x:Class="BcSoft.EDC.Surface.View.LoadProjectDataView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BcSoft.EDC.Surface.View"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        mc:Ignorable="d"
        Title="加载工程数据包" Height="800" Width="600" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TabControl>
            <TabItem Header="本地工程" Width="160">
                <ContentControl x:Name="m_LocalProjectView" />
            </TabItem>
            <TabItem Header="下载工程" Width="160">
                <ContentControl x:Name="m_DownProjectView"/>
            </TabItem>

        </TabControl>
    </Grid>
</dx:DXWindow>